---
import type { HTMLAttributes } from "astro/types";
import { input } from "./Input.css";

interface Props extends HTMLAttributes<"input"> {
  class?: string;
}

const { class: className, ...rest } = Astro.props;
---

<input
  type="text"
  class:list={[input, className]}
  autocomplete="off"
  {...rest}
/>
