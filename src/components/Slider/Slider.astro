---
import type { HTMLAttributes } from "astro/types";
import styles from "./Slider.module.css";
const { slider } = styles;

interface Props extends HTMLAttributes<"input"> {}

const { class: className, ...rest } = Astro.props;
---

<input
  class:list={["slider", slider, className]}
  type="range"
  autocomplete="off"
  {...rest}
/>
